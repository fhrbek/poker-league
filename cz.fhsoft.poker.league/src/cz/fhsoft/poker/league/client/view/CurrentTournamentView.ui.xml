<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:cell="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		.mainTabs {
			height: 100%;
		}
	</ui:style>
	<g:FlowPanel>
		<g:Label ui:field="tournamentName"/>
		<g:FlowPanel ui:field="newGame">
			<g:Label>V tuto chvíli není rozhrána žádná hra, můžete zahájit novou hru.</g:Label>
			<g:FlowPanel ui:field="newPlayersContainer"/>
			<g:Button ui:field="startNewGame">Zahájit novou hru</g:Button>
		</g:FlowPanel>
		<g:FlowPanel ui:field="currentGame">
			<g:Label ui:field="gameName"/>
			<g:FlowPanel ui:field="playersInGameContainer"/>
			<g:Button ui:field="seatOpen">Vyřadit označené hráče</g:Button>
			<g:Button ui:field="undoSeatOpen">Vrátit do hry označené hráče</g:Button>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 